<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="campusorders.com.quebasetech.joe.campusorders">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".utils.CampusOrder"
        android:allowBackup="true"
        android:icon="@drawable/rounded_input"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".BuyerHome"
            android:label="@string/app_name" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Settings_Profile"
            android:label="Settings"
            android:parentActivityName=".BuyerHome" />
        <activity
            android:name=".AccountSetup"
            android:label="Account Setup"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".HelpPage"
            android:parentActivityName=".Settings_Profile"
            android:label="Help"/>
        <activity
            android:name=".AboutPage"
            android:label="About App"
            android:parentActivityName=".Settings_Profile"/>
        <activity
            android:name=".AccountInfo"
            android:label="Account Info"
            android:parentActivityName=".Settings_Profile"/>
    </application>

</manifest>